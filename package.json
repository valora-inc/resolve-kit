{
  "name": "@valora/resolve-kit",
  "version": "1.0.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext=.tsx,.ts,.json --fix src/ scripts/",
    "format": "prettier --loglevel error --write \"./**/*.ts\"",
    "test": "jest --coverage && prettier --check \"./**/*.ts\" && eslint --ext=.tsx,.ts,.json src/ scripts/",
    "prepublishOnly": "yarn build",
    "release": "standard-version",
    "resolve": "ts-node --files=src/global.d.ts scripts/resolve.ts"
  },
  "repository": "git@github.com:valora-inc/resolve-kit.git",
  "author": "Valora Inc",
  "license": "Apache-2.0",
  "prettier": "@valora/prettier-config",
  "devDependencies": {
    "@celo/contractkit": "^3.0.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@valora/eslint-config-typescript": "^0.0.1",
    "@valora/prettier-config": "^0.0.1",
    "eslint": "^8.29.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.8.1",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "4.4.3",
    "yargs": "^17.3.1"
  },
  "dependencies": {
    "@celo/base": "^3.0.1",
    "@celo/utils": "^3.0.1",
    "@ensdomains/ensjs": "^2.1.0",
    "eth-ens-namehash": "^2.0.8",
    "ethers": "^5.7.2"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
